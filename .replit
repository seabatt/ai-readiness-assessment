run = "npm run dev"
language = "nodejs"
entrypoint = "src/app/page.tsx"
modules = ["nodejs-20", "postgresql-16"]

[nix]
channel = "stable-23_11"

[deployment]
run = ["npm", "start"]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 40521
externalPort = 3002

[[ports]]
localPort = 43355
externalPort = 3000

[[ports]]
localPort = 45221
externalPort = 3001

[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0", "javascript_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
